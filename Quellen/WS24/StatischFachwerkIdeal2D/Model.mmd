        +AddRod(nodeA: Node, nodeB: Node) Rod
        +Solve() void
    }
    class Node {
        +Name: string
        +X: double
        +Y: double
        +FixX: boolean
        +FixY: boolean
        +ForceX: double
        +ForceY: double
    }
    class Rod {
        +Force: double
    }

