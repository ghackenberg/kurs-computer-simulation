<Window x:Class="SFunctionContinuous.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SFunctionContinuous"
        xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
        xmlns:mlsagl="http://mlsagl"
        mc:Ignorable="d"
        Title="Einfache S-Funktion" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ScottPlot:WpfPlot x:Name="Chart" Grid.Row="0" Grid.Column="0"/>
        <mlsagl:AutomaticGraphLayoutControl x:Name="Graph" Grid.Row="0" Grid.Column="1"/>
    </Grid>
</Window>
